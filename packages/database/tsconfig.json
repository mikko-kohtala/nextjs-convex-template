{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": false,
    "strict": true,
    "outDir": "dist",
    "rootDir": "./",
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "skipLibCheck": true
  },
  "include": ["convex/**/*.ts", "src/**/*.ts"],
  "exclude": ["node_modules", "convex/_generated", "convex/convex.config.ts"]
}
